{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6" /* Set the JavaScript language version */,
    "module": "es6" /* Specify what module code is generated. */,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}
